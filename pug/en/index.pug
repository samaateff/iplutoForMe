extends ../layout
include ../components/mixins

append variable
  - 
    let page            = "home"
    let pageTitle       = "اي بلوتو - الرئيسية"
    lang                = "en"
    dir                 = "ltr"

block stylesheets
  // google fonts  
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;700&display=swap" rel="stylesheet">
  // Fontawesome 
  link(rel="stylesheet", href=`${siteDomin}/assets/css/all.min.css`)
  // Bootstrap 
  link(rel="stylesheet", href=`${siteDomin}/assets/css/bootstrap.min.css`)
  // swiper js
  link(rel="stylesheet" href=`${siteDomin}/assets/css/swiper.css`) 
  // Custom Stylesheet
  link(rel="stylesheet", href=`${siteDomin}/assets/css/style.css`)
  link(rel="stylesheet", href=`${siteDomin}/assets/css/ltr.css`)

block header
  header#header(class=headerClass)
    .container-lg
      nav.navbar.navbar-expand-lg
        a.navbar-brand(href=`${siteDomin}/`)
          //- This block use to : Change loge in header
          block headerImage
            img(src=`${siteDomin}/assets/images/ipluto-logo.webp`)
        button.navbar-toggler(type="button" data-bs-toggle="offcanvas" data-bs-target="#navbarHeader" aria-controls="navbarHeader")
          i.fa-solid.fa-bars
        .offcanvas.offcanvas-start#navbarHeader
          .offcanvas-header
            a.navbar-brand(href=`${siteDomin}/`)
              img(src=`${siteDomin}/assets/images/ipluto-logo.webp`)
            button.close-button(data-bs-dismiss="offcanvas" aria-label="Close" type="button")
              i.fa-solid.fa-xmark
          ul.navbar-nav
            li.nav-item
              a.nav-link(href=`${siteDomin}/` class=(page === "home" ? "active" : null)) Main
            li.nav-item
              a.nav-link(href=`${siteDomin}/teachers.html` class=(page === "teachers" ? "active" : null)) Teachers
            li.nav-item
              a.nav-link(href=`${siteDomin}/registration/sign-in.html`) My lessons
            li.nav-item
              a.nav-link(href=`${siteDomin}/registration/sign-in.html`) My appointments
            li.nav-item
              a.nav-link(href=`${siteDomin}/registration/sign-in.html`) Lieutenant
            li.nav-item
              a.nav-link(href=`${siteDomin}/contact-us.html` class=(page === "contact-us" ? "active" : null)) Connect with us
            li.nav-item
              a.nav-link.lang(href=`${siteDomin}/`) 
                img(src=`${siteDomin}/assets/images/icons/ar-flag.webp`, alt="Arabic flag")
                span عربي
          ul.sign-buttons 
            li
              a.main-button(href=`${siteDomin}/registration/sign-in.html`) sign in
            li
              a.second-button(href=`${siteDomin}/registration/sign-up.html`) Register

block footer 
  // start Footer
  footer#footer
    .container-lg
      article  
        figure.image 
          img(src=`${siteDomin}/assets/images/footer-logo.webp`, alt="ipluto logo")
      article 
        ul.navbar-nav
          li.nav-item
            a.nav-link(href=`${siteDomin}/` class=(page === "home" ? "active" : null)) Main
          li.nav-item
            a.nav-link(href=`${siteDomin}/teachers.html` class=(page === "teachers" ? "active" : null)) Teachers
          li.nav-item
            a.nav-link(href=`${siteDomin}/registration/sign-in.html`) My lessons
          li.nav-item
            a.nav-link(href=`${siteDomin}/registration/sign-in.html`) My appointments
          li.nav-item
            a.nav-link(href=`${siteDomin}/registration/sign-in.html`) Lieutenant
          li.nav-item
            a.nav-link(href=`${siteDomin}/contact-us.html` class=(page === "contact-us" ? "active" : null)) Connect with us
          li.nav-item
            a.nav-link.lang(href=`${siteDomin}/en`) 
              img(src=`${siteDomin}/assets/images/icons/en-flag.svg`, alt="English flag")
              span عربي
        article.footer-box
          ul.social-footer
            li
              a.link(href="https://twitter.com/", target="_blank") 
                i.fa-brands.fa-x-twitter
            li
              a.link(href="https://www.linkedin.com/", target="_blank") 
                i.fa-brands.fa-linkedin-in
            li
              a.link(href="https://www.facebook.com/", target="_blank") 
                i.fa-brands.fa-facebook-f
          ul.sign-buttons 
            li
              a.main-button(href=`${siteDomin}/registration/teacher-sign.html`) Teacher, register
            li
              a.second-button(href=`${siteDomin}/registration/sign-up.html`) Register
          p.copywrite All rights reserved to #[a(href=`${siteDomin}/`) e-Pluto] educational platform 2023
  // End Footer

//- content This page
block pageContent
  // Start Home Header section
  section.home-header.overflow-hidden
    .container-lg
      section.home-heading 
        h4.title The path to success and excellence starts from here
        h1.heading  Welcome to the #[span.special-word iPluto] educational platform
        section.home-scenes  
          img.image-scene(src=`${siteDomin}/assets/images/home-scene-1.webp`, alt="home scene")
          img.image-scene(src=`${siteDomin}/assets/images/home-scene-2.webp`, alt="home scene")
          img.image-scene(src=`${siteDomin}/assets/images/home-scene-3.webp`, alt="home scene")
          img.image-scene(src=`${siteDomin}/assets/images/home-scene-4.webp`, alt="home scene")
          img.image-scene(src=`${siteDomin}/assets/images/home-scene-5.webp`, alt="home scene")
          img.image-scene(src=`${siteDomin}/assets/images/home-scene-6.webp`, alt="home scene")
  // End Home Header section
  // Start main search section
  section
    .container-lg
      section.main-search 
        h2.search-heading Search for study materials, teachers, etc...
        form.register-form(action="")
          .search-box
            section.custom-select.dropdown
              label Educational level
              input#educational_level(type="hidden", name="educational_level")
              button.input-button(type="button" data-bs-toggle="dropdown" aria-expanded="false")
                span Select the academic stage
                i.fa-solid.fa-chevron-down
              ul.dropdown-menu
                - for (let i = 1; i <= 6; i++)
                    a.dropdown-item(data-select-value="secondary-school-2" href="#") Second year of secondary school (scientific - science)
            section.custom-select.dropdown
              label Specialization
              input#user_qualification(type="hidden", name="user_qualification")
              button.input-button(type="button" data-bs-toggle="dropdown" aria-expanded="false")
                span Choose your specialty
                i.fa-solid.fa-chevron-down
              ul.dropdown-menu
                - for (let i = 1; i <= 6; i++)
                    a.dropdown-item(data-select-value="secondary-school-2" href="#") Bachelor's degree in Specific Education
            section.custom-select.dropdown
              label Subject
              input#subject_teach(type="hidden", name="subject_teach")
              button.input-button(type="button" data-bs-toggle="dropdown" aria-expanded="false")
                span Choose the academic subject
                i.fa-solid.fa-chevron-down
              ul.dropdown-menu
                - for (let i = 1; i <= 6; i++)
                    a.dropdown-item(data-select-value="secondary-school-2" href="#") mathematics
            button.submit(type="submit") research
