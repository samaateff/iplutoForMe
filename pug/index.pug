extends layout
include components/mixins

append variable
  - 
    let page            = "home"
    let pageTitle       = "اي بلوتو - الرئيسية"

//- content This page
block pageContent
  // Start Home Header section
  section.home-header.overflow-hidden
    .container-lg
      section.home-heading 
        h4.title طريق النجاح والتفوق يبدأ من هنا
        h1.heading اهلا بيك في منصة #[span.special-word اي-بلوتو] التعليمية
        section.home-scenes  
          img.image-scene(src=`${siteDomin}/assets/images/home-scene-1.webp`, alt="home scene")
          img.image-scene(src=`${siteDomin}/assets/images/home-scene-2.webp`, alt="home scene")
          img.image-scene(src=`${siteDomin}/assets/images/home-scene-3.webp`, alt="home scene")
          img.image-scene(src=`${siteDomin}/assets/images/home-scene-4.webp`, alt="home scene")
          img.image-scene(src=`${siteDomin}/assets/images/home-scene-5.webp`, alt="home scene")
          img.image-scene(src=`${siteDomin}/assets/images/home-scene-6.webp`, alt="home scene")
  // End Home Header section
  // Start main search section
  section
    .container-lg
      section.main-search 
        h2.search-heading ابحث عن مواد دراسية, معلمين, الخ ...
        form.register-form(action="")
          .search-box
            section.custom-select.dropdown
              label المرحلة الدراسية
              input#educational_level(type="hidden", name="educational_level")
              button.input-button(type="button" data-bs-toggle="dropdown" aria-expanded="false")
                span اختر المرحلة الدراسية 
                i.fa-solid.fa-chevron-down
              ul.dropdown-menu
                - for (let i = 1; i <= 6; i++)
                    a.dropdown-item(data-select-value="secondary-school-2" href="#") الصف الثاني الثانوي ( علمي - علوم )
            section.custom-select.dropdown
              label التخصص
              input#user_qualification(type="hidden", name="user_qualification")
              button.input-button(type="button" data-bs-toggle="dropdown" aria-expanded="false")
                span اختر التخصص 
                i.fa-solid.fa-chevron-down
              ul.dropdown-menu
                - for (let i = 1; i <= 6; i++)
                    a.dropdown-item(data-select-value="secondary-school-2" href="#") بكالوريوس تربية نوعية
            section.custom-select.dropdown
              label المادة الدراسية
              input#subject_teach(type="hidden", name="subject_teach")
              button.input-button(type="button" data-bs-toggle="dropdown" aria-expanded="false")
                span اختر المادة الدراسية 
                i.fa-solid.fa-chevron-down
              ul.dropdown-menu
                - for (let i = 1; i <= 6; i++)
                    a.dropdown-item(data-select-value="secondary-school-2" href="#") رياضيات
            button.submit(type="submit") بحث
  // End main search section  
